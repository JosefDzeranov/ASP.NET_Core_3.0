@model List<User>

<div class="row">
    @await Html.PartialAsync("Menu")

    <div class="column">
        <table>
            <tr>
                <th>Список пользователей</th>
            </tr>
            <tr>

                <th>Логин (e-mail)</th>
                <th>Имя</th>
                <th>Фамилия</th>

            </tr>


            @if (Model != null)
            {
                for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                    <td>@Model[i].Login</td>
                    <td>@Model[i].Name</td>
                    <td>@Model[i].LastName</td>
                    </tr>
                }
            }
            <tr></tr>
        </table>
    </div>
</div>