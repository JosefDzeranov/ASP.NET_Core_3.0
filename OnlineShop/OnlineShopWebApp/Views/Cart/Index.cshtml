@model List<Product>

@if (Model.Count == 0)
{
    <h1>"Корзина пуста"</h1>

}
else
{
    <table>
        <tr>
        <thead> Наименование</thead>
        <thead>Количество</thead>
        <thead>Стоимость</thead>
        </tr>



        @foreach (var product in Model)
        {
            <tr>
                <td>
             
                    
                        @product.Name
                    

                </td>


                <td>@product.Number</td>
            </tr>



        }


    </table>
}
